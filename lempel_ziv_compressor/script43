#imprime n a minusculas y toma los tiempos

rm -f tiempos2.csv;
touch tiempos2.csv;
echo n, t_compresion, t_descompresion > tiempos2.csv || break;
echo -------------------------------- >> tiempos2.csv || break;
n=1; while :; do
      n="`expr $n + 1`"; 
      head -c 100000 </dev/urandom >/tmp/test.$n.u;
      ./tp1.exe -m list -i /tmp/test.$n.u -o /tmp/test.$n.d || break;
      echo >> tiempos2.csv;
      rm -f /tmp/test.$n.*;
      echo Ok: $n;
done; echo Error: $n